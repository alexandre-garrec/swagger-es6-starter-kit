{
  "restartable": "rs",
  "ignore": [
    ".git"
  ],
  "verbose": false,
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "api/",
    "app.js"
  ],
  "delay": "10000ms",
  "env": {
    "NODE_ENV": "development",
    "BABEL_ENV": "development"
  },
  "ext": "js json yaml"
}